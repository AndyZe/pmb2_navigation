<launch>
  <arg name="map" default="$(find reem_maps)/config"/>

  <!-- Setup map -->
  <node pkg="reem_maps" type="map_setup.py" name="map_setup">
    <param name="map" value="$(arg map)"/>
  </node>

  <!-- Run the map server -->
  <node name="map_server" pkg="map_server" type="map_server" args="$(arg map)/map.yaml"/>

  <!-- Run the vo map server --> 
  <include file="$(find pal_vo_server)/launch/vo_server.launch"/>
</launch>
