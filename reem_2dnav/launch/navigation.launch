<launch>
  <arg name="laser_ip"/>
  <!-- Run laser filters -->
   <include file="$(find reem_laser_sensors)/launch/laser.launch">
   <arg name="laser_ip" value="$(arg laser_ip)"/>
  </include>

  <!-- Run map server -->
   <include file="$(find reem_maps)/launch/map.launch">
  </include>

  <!--- Run AMCL localiztion --> 
  <include file="$(find reem_2dnav)/launch/amcl_diff.launch">
    <arg name="topic" value="/scan_filtered" />
  </include> 

  <!-- Run emergency stop -->
  <include file="$(find reem_2dnav)/launch/emergency_stop.launch"> 
  </include>
 
  <!-- Run the move_base --> 
  <include file="$(find reem_2dnav)/launch/planning_and_control.launch"> 
  </include>
</launch>

